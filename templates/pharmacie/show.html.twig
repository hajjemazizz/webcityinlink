{% extends 'baseback.html.twig' %}

{% block title %}Pharmacie{% endblock %}

{% block body %}
    <h1>Pharmacie</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdPharmacie</th>
                <td>{{ pharmacie.idPharmacie }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ pharmacie.nom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ pharmacie.adresse }}</td>
            </tr>
            <tr>
                <th>Contact</th>
                <td>{{ pharmacie.contact }}</td>
            </tr>
            <tr>
                <th>Note</th>
                <td>{{ pharmacie.note }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_pharmacie_index') }}">back to list</a>

    <a href="{{ path('app_pharmacie_edit', {'idPharmacie': pharmacie.idPharmacie}) }}">edit</a>

    {{ include('pharmacie/_delete_form.html.twig') }}
